<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<HBox id ="moduleinfopane" fx:id="moduleinfopane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <GridPane HBox.hgrow="ALWAYS">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150"/>
        </columnConstraints>
        <VBox alignment="CENTER_LEFT" minHeight="150" GridPane.columnIndex="0">
            <padding>
                <Insets top="5" right="5" bottom="5" left="15" />
            </padding>
            <HBox spacing="5" alignment="CENTER_LEFT">
                <Label fx:id="id" styleClass="cell_big_label">
                    <minWidth>
                        <Region fx:constant="USE_PREF_SIZE" />
                    </minWidth>
                </Label>
                <Label fx:id="moduleinfocode" text ="\$moduleinfocode" styleClass="cell_big_label"/>
            </HBox>
            <Label fx:id="moduleinfotitle" text="\$moduleinfotitle" styleClass="cell_small_label"/>
            <HBox spacing="5" alignment="CENTER_LEFT">
                <Label fx:id="moduleinfodepartment" text="\$moduleinfodepartment" styleClass="cell_small_label"/>
                <Label fx:id="moduleinfocredits" text="\$moduleinfocredits" styleClass="cell_small_label"/>
            </HBox>
            <padding>
                <Insets top="5" right="5" bottom="5" left="15"/>
            </padding>
            <Label fx:id="moduleinfodecription" text="\$moduleinfodescription" styleClass="cell_small_label"/>
            <padding>
                <Insets top="5" right="5" bottom="5" left="15"/>
            </padding>
            <Label fx:id="moduleinfoprerequisites" text="\$moduleinfoprerequisite" styleClass="cell_small_label"/>
            <Label fx:id="moduleinfoworkload" text="\$moduleinfoworkload" styleClass="cell_small_label"/>
        </VBox>
    </GridPane>
</HBox>
